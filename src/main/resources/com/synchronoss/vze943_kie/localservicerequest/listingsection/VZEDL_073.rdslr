rule "VZEDL_073" dialect "mvel" ruleflow-group "DL_FORM"

when
		xcb object
 	 Evaluate 
Start Brace
 the field_tag "/xml/localservicerequest/directorylistingform" is exists 
		
AND
 The_Rule "VZEDL_073" multiple times "listinginstructionsection/ltn" in "/xml/localservicerequest/directorylistingform/listingsection[i]" is populated
		
