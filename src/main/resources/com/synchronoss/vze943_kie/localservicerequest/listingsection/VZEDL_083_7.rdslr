rule "VZEDL_083_7" dialect "mvel" ruleflow-group "DL_FORM"

when
		xcb object
 	 Evaluate 
Start Brace
 the field_tag "/xml/localservicerequest/directorylistingform" is exists 
		
AND
 The_Rule "VZEDL_083_7" multiple times "listinginstructionsection/lnln" in "/xml/localservicerequest/directorylistingform/listingsection[i]" is populated
		
