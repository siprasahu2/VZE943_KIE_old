AND
 The_Rule "VZERS_011_1" multiple times "otn" in "/xml/localservicerequest/resaleserviceform/servicedetailsection[i]" is_not populated
		
AND
 The_Rule "VZERS_011_1" multiple times "blocking[1]/block" in "/xml/localservicerequest/resaleserviceform/servicedetailsection[i]" is_not populated
		
AND
 The_Rule "VZERS_011_1" multiple times "tsp" in "/xml/localservicerequest/resaleserviceform/servicedetailsection[i]" is_not populated
		
AND
 The_Rule "VZERS_011_1" multiple times "features[1]/fa" in "/xml/localservicerequest/resaleserviceform/servicedetailsection[i]" is_not populated
		
AND
 The_Rule "VZERS_011_1" multiple times "lscp" in "/xml/localservicerequest/resaleserviceform/servicedetailsection[i]" is_not populated
